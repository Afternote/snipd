{
  "name": "snipd",
  "scripts": {
    "build_notes": "npx vite build ./notes_page --outDir=../extension/notes_page",
    "build_popup": "npx vite build ./popup --outDir=../extension/popup",
    "buildall": "npm run build_notes && npm run build_popup"
  },
  "workspaces": [
    "popup",
    "notes_page",
    "extension"
  ],
  "dependencies": {
    "@mantine/ds": "^6.0.0",
    "@mui/icons-material": "^5.11.11",
    "@tabler/icons": "^2.7.0",
    "@tabler/icons-react": "^2.47.0"
  },
  "devDependencies": {
    "prettier": "^2.8.4"
  },
  "prettier": {
    "printWidth": 100,
    "bracketSameLine": true
  }
}
